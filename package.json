{
  "name": "natours",
  "version": "1.0.0",
  "description": "Landing page for Natours",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/icon-font.css css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.concat.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css",
    "start": "node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tkoriginal/tourismSitewithAdvancedCSS.git"
  },
  "author": "Tauqueer Khan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tkoriginal/tourismSitewithAdvancedCSS/issues"
  },
  "homepage": "https://github.com/tkoriginal/tourismSitewithAdvancedCSS#readme",
  "devDependencies": {
    "autoprefixer": "^9.1.0",
    "concat": "^1.0.3",
    "node-sass": "^4.5.3",
    "nodemon": "^1.18.0",
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^6.0.0"
  },
  "dependencies": {
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "live-server": "^1.2.0"
  },
  "keywords": [
    "Advanced",
    "CSS",
    "Project"
  ]
}
